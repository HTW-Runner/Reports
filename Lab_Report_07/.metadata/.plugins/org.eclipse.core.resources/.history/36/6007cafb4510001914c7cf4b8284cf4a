package lab7;

import lab7.LinkedList.Node;

public class Node<T> {
	private T data;
	Node<T> next;

	public Node(T data, Node<T> next) {
		this.data = data;
		this.next = next;
	}

	public Node(T data) {
		this.data = data;
		this.next = null;
	}

	public T getValue() {
		return data;
	}

	public void setValue(T data) {
		this.data = data;
	}
	
	public Node<T> getNext() {
		return next;
	}

	public void setNext(Node<T> next) {
		this.next = next;
	}
}