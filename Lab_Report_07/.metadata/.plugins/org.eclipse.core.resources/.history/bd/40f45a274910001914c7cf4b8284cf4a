package lab7;

public class Node<T> {
	private T data;
	Node<T> next;

	public Node(T data, Node<T> next) {
		this.data = data;
		this.next = next;
	}

	public Node(T data) {
		this.data = data;
		this.next = null;
	}

	public T getValue() {
		return data;
	}

	public void setValue(T data) {
		this.data = data;
	}
}