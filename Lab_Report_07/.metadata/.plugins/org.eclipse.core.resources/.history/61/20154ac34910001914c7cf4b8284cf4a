package lab7;

public class Node<T> {
	private T data;
	Node<T> next;
	Node<T> prev;

	public Node(T data, Node<T> next, Node<T> prev) {
		this.data = data;
		this.next = next;
		this.prev = prev;
	}

	public Node(T data, Node<T> prev) {
		this.data = data;
		this.next = null;
		this.prev = prev;
	}

	public T getValue() {
		return data;
	}

	public void setValue(T data) {
		this.data = data;
	}
}